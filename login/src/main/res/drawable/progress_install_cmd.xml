<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >


    <item android:id="@android:id/background">
        <shape>
            <gradient
                android:centerColor="#D1FDDC"
                android:centerX="1.0"
                android:endColor="#D1FDDC"
                android:startColor="#D1FDDC" />
        </shape>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:centerColor="#5EEE9A"
                    android:centerY="0.5"
                    android:endColor="#5EEE9A"
                    android:startColor="#5EEE9A"
                    android:type="linear" />
            </shape>
        </clip>
    </item>

</layer-list>
